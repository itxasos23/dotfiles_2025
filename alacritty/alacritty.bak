import:
  - ~/.alacritty-colorscheme/themes/remedy_dark.yaml


font:
  normal:
    family: Hack Nerd Font Mono
    style: Regular 
  bold:
    family: Hack Nerd Font Mono
    style: Bold 
  italic:
    family: Hack Nerd Font Mono
    style: Italic 
  bold_italic:
    family: Hack Nerd Font Mono
    style: Bold Italic
  size: 10.0
  offset:
    x: 0
    y: 0
  glyph_offset:
    x: 0
    y: 0

shell:
  program: /usr/bin/fish 
